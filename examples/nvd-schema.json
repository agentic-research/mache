{
  "version": "v1",
  "nodes": [
    {
      "name": "by-cve",
      "selector": "$",
      "children": [
        {
          "name": "{{.item.cve.id}}",
          "selector": "$[*]",
          "files": [
            {
              "name": "description",
              "content_template": "{{with (index .item.cve.descriptions 0)}}{{.value}}{{end}}"
            },
            {
              "name": "published",
              "content_template": "{{.item.cve.published}}"
            },
            {
              "name": "status",
              "content_template": "{{.item.cve.vulnStatus}}"
            },
            {
              "name": "raw.json",
              "content_template": "{{. | json}}"
            }
          ]
        }
      ]
    }
  ]
}
